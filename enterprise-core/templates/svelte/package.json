{
  "name": "tiation-svelte-enterprise-template",
  "version": "1.0.0",
  "description": "Enterprise-grade SvelteKit application template with Tiation branding and dark neon theme",
  "author": "Tiation Team <tiatheone@protonmail.com>",
  "homepage": "https://tiation.github.io/svelte-enterprise-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiation/svelte-enterprise-template.git"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "template",
    "enterprise",
    "tiation",
    "dark-theme",
    "responsive",
    "typescript",
    "tailwindcss"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@iconify/json": "^2.2.201",
    "@skeletonlabs/skeleton": "^2.9.1",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.30.4",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.14",
    "svelte-check": "^3.6.9",
    "tailwindcss": "3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3",
    "vite-plugin-tailwind-purgecss": "0.3.1",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
    "highlight.js": "11.9.0",
    "unplugin-icons": "^0.18.5"
  }
}
