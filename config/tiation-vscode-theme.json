{
  "name": "Tiation Dark Neon",
  "type": "dark",
  "colors": {
    "activityBar.background": "#000000",
    "activityBar.foreground": "#00ffff",
    "activityBarBadge.background": "#ff00ff",
    "activityBarBadge.foreground": "#0a0a0a",
    "badge.background": "#00ffff",
    "badge.foreground": "#0a0a0a",
    "button.background": "#00ffff",
    "button.foreground": "#0a0a0a",
    "button.hoverBackground": "#00ff41",
    "editor.background": "#0a0a0a",
    "editor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editor.selectionBackground": "#00ffff40",
    "editorCursor.foreground": "#00ffff",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorIndentGuide.background": "#1a1a1a",
    "editorIndentGuide.activeBackground": "#00ffff",
    "editorLineNumber.foreground": "#808080",
    "editorLineNumber.activeForeground": "#00ffff",
    "panel.background": "#000000",
    "panel.border": "#00ffff",
    "panelTitle.activeForeground": "#00ffff",
    "sideBar.background": "#000000",
    "sideBar.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#1a1a1a",
    "sideBarSectionHeader.foreground": "#00ffff",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#00ffff",
    "statusBarItem.hoverBackground": "#00ffff40",
    "tab.activeBackground": "#0a0a0a",
    "tab.activeForeground": "#00ffff",
    "tab.inactiveBackground": "#000000",
    "tab.inactiveForeground": "#b0b0b0",
    "terminal.background": "#0a0a0a",
    "terminal.foreground": "#ffffff",
    "terminal.ansiCyan": "#00ffff",
    "terminal.ansiMagenta": "#ff00ff",
    "terminal.ansiGreen": "#00ff41",
    "terminal.ansiBlue": "#0080ff",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#00ffff",
    "workbench.colorTheme": "Tiation Dark Neon"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": [
        "variable",
        "parameter"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#8000ff"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#0080ff",
        "fontStyle": "bold"
      }
    }
  ]
}